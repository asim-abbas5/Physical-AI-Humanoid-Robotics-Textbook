{
  "name": "physical-ai-humanoid-robotics-textbook",
  "version": "1.0.0",
  "description": "AI-native textbook for Physical AI and Humanoid Robotics",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "server": "tsx watch server/index.ts",
    "server:build": "tsc --project server/tsconfig.json",
    "server:init-db": "tsx server/scripts/init-db.ts",
    "dev": "concurrently \"npm run server\" \"npm run start\"",
    "lint": "npm run lint:md && npm run lint:prose",
    "lint:md": "markdownlint-cli2 \"**/*.{md,mdx}\"",
    "lint:prose": "echo 'Vale not configured yet'",
    "lint:fix": "markdownlint-cli2 --fix \"**/*.{md,mdx}\" && prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "validate:all": "npm run lint && echo 'Python validation scripts not yet implemented'"
  },
  "dependencies": {
    "@docusaurus/core": "^3.0.0",
    "@docusaurus/plugin-ideal-image": "^3.0.0",
    "@docusaurus/preset-classic": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "better-auth": "^1.4.5",
    "body-parser": "^2.2.1",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "pg": "^8.16.3",
    "prism-react-renderer": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.0.0",
    "@docusaurus/types": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/node": "^24.10.1",
    "concurrently": "^9.2.1",
    "markdownlint-cli2": "^0.12.0",
    "prettier": "^3.1.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "18.x"
  }
}
